{
  "name": "@taiyme/eslint-config",
  "version": "1.0.0",
  "private": false,
  "description": "taiyのためのESLint共有設定",
  "homepage": "https://github.com/taiyme/eslint-config",
  "bugs": "https://github.com/taiyme/eslint-config/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyme/eslint-config.git"
  },
  "license": "MIT",
  "author": "taiy <me@taiy.me> (https://taiy.me/)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ./scripts/typegen.ts && tsdown",
    "clean": "node ./scripts/clean.ts && pnpm store prune",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@eslint/js": "9.39.2",
    "@stylistic/eslint-plugin": "5.9.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-perfectionist": "5.2.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-unused-imports": "4.3.0",
    "typescript-eslint": "8.51.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/node": "24.10.4",
    "@typescript-eslint/parser": "8.51.0",
    "better-typescript-lib": "2.12.0",
    "eslint": "9.39.2",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-typegen": "2.3.0",
    "globals": "17.0.0",
    "jiti": "2.6.1",
    "tsdown": "0.18.4",
    "type-fest": "5.3.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "eslint": ">=9.18.0",
    "typescript": ">=5.5.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    },
    "typescript": {
      "optional": false
    }
  },
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
